<?xml version="1.0"?>
<features>
	<feature>
		<options>number</options>
		<optionstype>Constant</optionstype>
		<natural>$value</natural>
		<code>$value</code>
		<help>http://www.help.com/constant</help>
		<parameters>
			<parameter>
				<name>value</name>
				<type>INTEGER</type>
				<default>0</default>
			</parameter>
		</parameters>
	</feature>
	<feature>
		<options>number</options>
		<optionstype>DecimalConstant</optionstype>
		<natural>$value</natural>
		<code>$value</code>
		<help>http://www.help.com/decimal</help>
		<parameters>
			<parameter>
				<name>value</name>
				<type>REAL</type>
				<default>0.0</default>
			</parameter>
		</parameters>
	</feature>
	<feature>
		<options>comparison</options>
		<optionstype>HigherThan</optionstype>
		<natural>is higher than </natural>
		<code> &gt; </code>
		<help>http://www.help.com/higherThan</help>		
	</feature>
	<feature>
		<options>comparison</options>
		<optionstype>LowerThan</optionstype>
		<natural>is lower than </natural>
		<code> &lt; </code>
		<help></help>		
	</feature>
	<feature>
		<options>comparison</options>
		<optionstype>HigherThanOrEqualTo</optionstype>
		<natural>is higher than or equal to </natural>
		<code> &gt;= </code>
		<help></help>		
	</feature>
	<feature>
		<options>comparison</options>
		<optionstype>LowerThanOrEqualTo</optionstype>
		<natural>is lower than or equal to </natural>
		<code> &lt;= </code>
		<help></help>		
	</feature>
	<feature>
		<options>comparison</options>
		<optionstype>EqualTo</optionstype>
		<natural>is equal to </natural>
		<code> == </code>
		<help></help>		
	</feature>
	<feature>
		<options>basicfunction</options>
		<optionstype>UserAge</optionstype>
		<natural>Script1 User Age $pscript2 years ago </natural>
		<code>Agescript3[$pscript2]</code>
		<help>http://www.help.com/age</help>
		<parameters>
			<parameter>
				<name>pscript2</name>
				<type>INTEGER</type>
				<default>0</default>
			</parameter>
		</parameters>
	</feature>
	<feature>
		<options>advancedfunction</options>
		<optionstype>born</optionstype>
		<natural>Script1 Born in Year $datebornscript2</natural>
		<code>AdvancedAgescript3($countryscript4, $colorscript5, $datebornscript2)</code>
		<help>http://www.help.com/born</help>
		<parameters>
			<parameter>
				<name>countryscript4</name>
				<type>text</type>
				<allowedvalues>
					<value>USA</value>
					<value>CAN</value>
					<value>EUR</value>
				</allowedvalues>
				<default>USA</default>
				<allowvar>No</allowvar>
			</parameter>
			<parameter>
				<name>colorscript5</name>
				<type>text</type>
				<allowedvalues>
					<value>White</value>
					<value>Black</value>
					<value>Green</value>
				</allowedvalues>
				<default>White</default>
				<allowvar>No</allowvar>
			</parameter>
			<parameter>
				<name>datebornscript2</name>
				<type>INTEGER</type>
				<interval>
					<lowerlimit>1950</lowerlimit>
					<upperlimit>2012</upperlimit>
				</interval>
				<default>1960</default>
				<allowvar>Yes</allowvar>
			</parameter>
		</parameters>
	</feature>
	<feature>
		<options>advancedfunction</options>
		<optionstype>height</optionstype>
		<natural>Script1 User Height of $heigscript2</natural>
		<code>Heightscript3($heigscript2)</code>
		<help></help>
		<parameters>
			<parameter>
				<name>heigscript2</name>
				<type>REAL</type>
				<interval>
					<lowerlimit>4.0</lowerlimit>
					<upperlimit>8.0</upperlimit>
				</interval>
				<default>5.1</default>
				<allowvar>Yes</allowvar>
			</parameter>			
		</parameters>
	</feature>
	<feature>
		<options>externaloption</options> 
		<optionstype>CustombyUser</optionstype> 
		<natural>custombyuser($value,$country,$external)</natural> 
		<code>custombyuser($value,$country,$external)</code> 
		<help>http://www.help.com/age</help> 
		<parameters>
			<parameter>
				<name>value</name> 
				<type>INTEGER</type> 
				<default>0</default> 
			</parameter>
			<parameter>
				<name>country</name> 
				<type>text</type> 
				<allowedvalues>
					<value>USA</value> 
					<value>CAN</value> 
					<value>EUR</value> 
				</allowedvalues>
				<default>USA</default> 
				<allowvar>No</allowvar> 
			</parameter>

			<parameter> 
				<name>external</name> 
				<type>text</type> 
				<default></default> 
				<allowvar>No</allowvar> 
			</parameter> 
		</parameters>
	</feature>
	<feature>
		<options>action</options>
		<optionstype>run</optionstype>
		<natural>runscript1 on $dayscript2</natural>
		<code>Runscript3($dayscript2)</code>
		<help></help>
		<parameters>
			<parameter>
				<name>dayscript2</name>
				<type>text</type>
				<allowedvalues>
					<value>Monday</value>
					<value>Tuesday</value>
					<value>Wednesday</value>
					<value>Friday</value>
				</allowedvalues>
				<default>Monday</default>
				<allowvar>No</allowvar>
			</parameter>			
		</parameters>
	</feature>
	<feature>
		<options>action</options>
		<optionstype>sleep</optionstype>
		<natural>sleepscript1 on $dayscript2 at $hourscript3</natural>
		<code>Sleepscript4($dayscript2,$hourscript3)</code>
		<help></help>
		<parameters>
			<parameter>
				<name>dayscript2</name>
				<type>text</type>
				<allowedvalues>
					<value>Monday</value>
					<value>Tuesday</value>
					<value>Wednesday</value>
					<value>Thursday</value>
				</allowedvalues>
				<default>Monday</default>
				<allowvar>No</allowvar>
			</parameter>
			<parameter>
				<name>hourscript3</name>
				<type>INTEGER</type>
				<interval>
					<lowerlimit>0</lowerlimit>
					<upperlimit>24</upperlimit>
				</interval>
				<default>20</default>
				<allowvar>Yes</allowvar>
			</parameter>			
		</parameters>
	</feature>        
</features>